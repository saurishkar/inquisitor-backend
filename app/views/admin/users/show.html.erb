<%= render 'admin/home/header' %>
<div class="container">
  <h1>User Profile</h1>
  <div class="row">
    <div class="col-md-6">
      <h4>First Name</h4>
      <p><%= @user.firstname %></p>
    </div>
    <div class="col-md-6">
      <h4>Last Name</h4>
      <p><%= @user.lastname %></p>
    </div>
  </div>
  <div  class="row">
    <div class="col-md-6">
      <h4>Email Address</h4>
      <p><%= @user.email %></p>
    </div>
  </div>
</div>